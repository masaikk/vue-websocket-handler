{
  "name": "vue-websocket-handler",
  "version": "0.0.1",
  "description": "an vue websocket handler",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masaikk/vue-websocket-handler.git"
  },
  "keywords": [
    "vue",
    "websocket",
    "hooks"
  ],
  "author": "masaikk",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "prettier": "^2.8.3",
    "ts-loader": "^9.4.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vue": "^3.2.47",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
