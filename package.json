{
  "name": "vue-websocket-handler",
  "version": "0.0.4",
  "description": "a vue websocket handler library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masaikk/vue-websocket-handler.git"
  },
  "keywords": [
    "vue",
    "websocket",
    "hooks"
  ],
  "author": "masaikk",
  "license": "LGPL",
  "devDependencies": {
    "prettier": "^2.8.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vue": "^3.2.47"
  }
}
